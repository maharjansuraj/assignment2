    });
    ga('webTracker.require', 'ec');

    var newsId = 'M7gh5J0i5i';

    if(newsId != ""){

        ga('webTracker.ec:addProduct', {

            'id':newsId,                        // 新闻的ID （string），id或name必需设置其一
